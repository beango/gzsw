@using gzsw.web
@{
    Layout = null;
} 
<!DOCTYPE html> 
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no" />
    <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
    <meta name="keywords" content="@ViewBag.Keywords" />
    <meta name="description" content="@ViewBag.Description" />
    @Html.JavaScript("lib/utility/core.js")
    <!-- Jquery 包 -->
    @Html.JavaScript("lib/jquery/jquery-1.7.1.min.js")
    <!--  弹出框插件 -->
    @Html.JavaScript("lib/layer/layer.min.js")
    @Html.JavaScript("lib/utility/gzsw.dialog.js")
    @Html.Raw(TempData["alert"])
    @RenderSection("Head", false)
    <!-- Bootstrap 样式 -->
   @*  @Html.StyleSheet("bootstrap/css/bootstrap.min.css")
    @Html.StyleSheet("bootstrap/css/bootstrap-theme.min.css")
   
    @Html.StyleSheet("bootstrap/css/gzsw_reset.css")    *@ 
</head>
<body>
    @RenderBody()
</body>
</html>
