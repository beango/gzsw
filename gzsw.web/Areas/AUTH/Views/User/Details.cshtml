    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@section Head{ 
} 
    <table class="formTable" cellpadding="0" cellspacing="0">
        <tr>
            <td style="width:100px;" class="tdLeft"> 用户账号：</td>
            <td>@Html.DisplayFor(model => model.USER_ID)</td>
