@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<table class="formTable" cellpadding="0" cellspacing="0">
        <tr>
            <td style="width:100px;" class="tdLeft">
            标题：
            </td>
            <td>
                @Model.TITLE
            </td>
        </tr>

        <tr>
            <td class="tdLeft">
                发送对象：
            </td><td>
            @Model.ObjNam
            @Html.Raw((Model.MT == 1 ? (Model.CLI_READ_IND != null && Model.CLI_READ_IND ? "（已读）" : "<font color='red'>（未读）</font>") : ""))
            </td>
        </tr>

        <tr>
            <td class="tdLeft">
                信息详情：
            </td><td>
                @Model.RELEASE_MESSAGE
            </td>
        </tr>

        <tr>
            <td class="tdLeft">
                开始有效时间：
            </td><td>
                @Model.BEGIN_TIME.ToString("yyyy-MM-dd HH:mm:ss")</td>
        </tr>

        <tr>
            <td class="tdLeft">
                结束有效时间：
            </td><td>
                @Model.END_TIME.ToString("yyyy-MM-dd HH:mm:ss")</td>
        </tr>

    </table>

