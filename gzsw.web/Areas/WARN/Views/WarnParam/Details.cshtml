@section Head
{
    <script lang="javascript">
        $(function () {
            // 注册事件
            // 查询
            $("#btnSearch").ligerButton(
            {
                text: " 查 询 ",
                click: function () {
                    var url = "?HALLNAM=" + $("#txtHALLNAM").val();
                    location.href = url.urlstamp();
                },
                icon: "@Html.Url("Content/lib/ligerUI/skins/icons/search.png")"
            });
            // 顶部导航
            var bar = $("#toptoolbar").ligerToolBar();

            bar.addItem(@Html.AuthViewButton("WARN_PARAM_VIW",
            Url.Content("ParamSendUsr") + "?hallno=" + Request.QueryString["hallno"],
            "预警发送人配置", 500, 400));
            bar.addItem({
                text: '修改',
                click: function (item) {
                    var url = '@Url.Content("Create")?hallno=@Request.QueryString["hallno"]';
                    location.href = url;
                },
                icon: 'archives'
            });

        });
    </script>

    <script lang="javascript">
        $(function () {
            $(".tablelist a[name=viewModel]").each(
                function () {
                    $(this).ligerTip({ content: $(this).next().html(), width: 110, auto: true });
                });
        });
    </script>
}