@{
    ViewBag.Title = "明细";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@using gzsw.util.Extensions
@model dynamic


<table class="formTable" cellpadding="0" cellspacing="0">
    <tr>
        <td class="tdLeft">服务厅:</td>
        <td>
            @Model.YWHALLNAME
        </td>
        <td class="tdLeft">窗口:</td>
        <td>
            @Model.COUNTER
        </td>
    </tr>
    <tr>
        <td class="tdLeft">排队号码:</td>
        <td>
            @Model.NUMBER
        </td>
        <td class="tdLeft">排队业务:</td>
        <td>
            @Model.QSERIALNAME
        </td>
    </tr>
    <tr>
        <td class="tdLeft">大类业务:</td>
        <td>
            @Model.DLSSERIALNAME
        </td>
        <td class="tdLeft">业务序号:</td>
        <td>
            @Model.XH
        </td>
    </tr>
    <tr>
        <td class="tdLeft">明细业务:</td>
        <td>
            @Model.SERIALNAME
        </td>
        <td class="tdLeft">受理员工:</td>
        <td>
            @Model.STAFFNAME
        </td>
    </tr>
    <tr>
        <td class="tdLeft">开始受理时间:</td>
        <td>
            @(Model.STIME == null ? string.Empty : Model.STIME.ToString("yyyy-MM-dd HH:mm:ss"))
        </td>
        <td class="tdLeft">结束受理时间:</td>
        <td>
            @(Model.ETIME == null ? string.Empty : Model.ETIME.ToString("yyyy-MM-dd HH:mm:ss"))
        </td>
    </tr>
    <tr>
        <td class="tdLeft">受理总时长:</td>
        <td>
            @(((int)Model.BLTIME).ToTimeString())
        </td>
        <td class="tdLeft">纳税人识别码:</td>
        <td>
            @Model.NSRSBM
        </td>
    </tr>
    <tr>
        <td class="tdLeft">纳税人名称:</td>
        <td>
            @Model.NSRMC
        </td>
        <td class="tdLeft">纳税机构:</td>
        <td>
            @Model.BELHALLNAME
        </td>
    </tr>
</table>