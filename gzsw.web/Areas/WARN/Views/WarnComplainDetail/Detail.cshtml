@using gzsw.util
@model gzsw.model.WARN_COMPLAIN_DETAIL

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
 <table class="formTable" cellpadding="0" cellspacing="0">
        <tr>
            <td style="width:200px;" class="tdLeft">
            纳税人身份证/企业识别码：
            </td>
            <td>
                @Model.NSR_SBM
            </td>
        </tr>

        <tr>
            <td class="tdLeft">
               投诉所属服务厅：
            </td><td>
            @ViewBag.HallName 
            </td>
        </tr>
     
        <tr>
            <td class="tdLeft">
               投诉员工：
            </td><td>
            @ViewBag.StaffName 
            </td>
        </tr>
        <tr>
            <td class="tdLeft">
                投诉问题描述：
            </td><td>
                @Model.COMPLAIN_PRO
            </td>
        </tr>

        <tr>
            <td class="tdLeft">
                投诉类型名称：
            </td><td>
                @Model.COMPLAIN_NAM

                 </td>
        </tr>

        <tr>
            <td class="tdLeft">
                投诉时间：
            </td><td>
                @Model.COMPLAIN_TIME.ToString("yyyy-MM-dd HH:mm:ss")

                 </td>
        </tr>
     <tr>
         <td class="tdLeft">
             处理状态：
         </td><td> 
               @(EnumHelper.ConvertToE<gzsw.model.ext.WARN_COMPLAIN_DETAIL.DETAIL_STATUS_ENUM>(Model.STATE.ToString()))

              </td>
     </tr> 
     <tr>
         <td class="tdLeft">
             处理人：
         </td><td>
                  @Model.HANDLE_USER</td>
     </tr>
     <tr>
         <td class="tdLeft">
             处理时间：
         </td><td>
                  @Model.HANDLE_TIME</td>
     </tr> 
     <tr>
         <td class="tdLeft">
             投诉原因：
         </td><td>
                  @Model.COMPLAIN_REASON</td>
     </tr>
     <tr>
         <td class="tdLeft">
             投诉处理：
         </td><td>
                  @Model.COMPLAIN_METHOD</td>
     </tr>
     <tr>
         <td class="tdLeft">
             录入人：
         </td><td>
                  @Model.INPUT_USER</td>
     </tr>
     <tr>
            <td class="tdLeft">
                录入时间：
            </td><td>
                @Model.INPUT_TIME</td>
        </tr>
    </table>
