@{
    ViewBag.Title = "查看业务量数据修正";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@using gzsw.controller.CHK.Models
@using gzsw.model
@using gzsw.util
@using gzsw.util.Extensions
@using gzsw.web
@model gzsw.model.Subclasses.STAT_STAFF_SVRSTAT_M_SUB



<table class="formTable" cellpadding="0" cellspacing="0">
    <tr>
        <td style="width: 160px;" class="tdLeft">员工名称:</td>
        <td >
            @Html.DisplayFor(m => m.STAFF_NAM)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">月份:</td>
        <td>
            @Model.STAT_MO.ToYearsMonthString()
        </td>
    </tr>
    <tr>
        <td style="width: 160px;" class="tdLeft">业务事项:</td>
        <td>
            @Html.DisplayFor(m => m.SERIALNAME)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">上门业务笔数:</td>
        <td>
            @Html.DisplayFor(m => m.COR_DOOR_SVR_CNT)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">其他业务笔数:</td>
        <td>
            @Html.DisplayFor(m => m.OTHER_SVR_CNT)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">超时业务笔数:</td>
        <td>
            @Html.DisplayFor(m => m.COR_OVERTIME_SVR_CNT)
        </td>
    </tr>
   
</table>