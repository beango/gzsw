@{
    ViewBag.Title = "查看满意度";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@using gzsw.web
@model gzsw.model.Subclasses.CHK_STAFF_ATTEND_DEDUCT_CON_SUB


<table class="formTable" cellpadding="0" cellspacing="0">
    <tr>
        <td style="width: 160px;" class="tdLeft">组织结构：</td>
        <td>
            @Html.DisplayFor(m => m.ORG_NAM)
            @Html.HiddenFor(m => m.ORG_ID)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">早退扣分：</td>
        <td>
            @Html.DisplayFor(m => m.EAR_SCORE)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">旷工扣分：</td>
        <td>
            @Html.DisplayFor(m => m.NEG_SCORE)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">迟到扣分：</td>
        <td>
            @Html.DisplayFor(m => m.LAT_SCORE)
        </td>
    </tr>
    
</table>
