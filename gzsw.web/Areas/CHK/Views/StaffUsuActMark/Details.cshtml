@{
    ViewBag.Title = "查看日常行为表现";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@using gzsw.controller.CHK.Models
@using gzsw.model
@using gzsw.model.Enums
@using gzsw.util
@using gzsw.util.Extensions
@using gzsw.web
@model gzsw.controller.CHK.Models.StaffUsuActMarkModel



<table class="formTable" cellpadding="0" cellspacing="0">
    <tr>
        <td class="tdLeft">服务厅：</td>
        <td>
            @Html.HiddenFor(m => m.ORG_ID)
            @Html.HiddenFor(m => m.SEQ)
            @Html.DisplayFor(m => m.ORG_NAM)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">员工名称:</td>
        <td>
            @Html.HiddenFor(m => m.STAFF_ID)
            @Html.DisplayFor(m => m.STAFF_NAM)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">月份:</td>
        <td>
            @Html.DisplayFor(m => m.STAT_MO)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">行为类型:</td>
        <td>
           @(((USUACTTYPEENUM)Model.USU_ACT_TYP).GetDescription())
        </td>
    </tr>
    <tr>
        <td class="tdLeft">扣分:</td>
        <td>
            @Html.DisplayFor(m => m.DEDUCT)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">原因:</td>
        <td>
            @Html.DisplayFor(m => m.REASON)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">打分时间:</td>
        <td>
            @Model.MARK_TIME.ToString("yyyy-MM-dd HH:mm:ss")
        </td>
    </tr>
</table>