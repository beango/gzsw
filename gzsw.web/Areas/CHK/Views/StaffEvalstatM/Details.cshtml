@{
    ViewBag.Title = "查看纳税人评价表现修正";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@using gzsw.util.Extensions
@model gzsw.model.Subclasses.STAT_STAFF_EVALSTAT_M_SUB



<table class="formTable" cellpadding="0" cellspacing="0">
    <tr>
        <td style="width: 160px;" class="tdLeft">员工名称:</td>
        <td>
            @Html.DisplayFor(m => m.STAFF_NAM)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">月份:</td>
        <td>
            @Model.STAT_MO.ToYearsMonthString()
        </td>
    </tr>
    <tr>
        <td class="tdLeft">评价很满意量:</td>
        <td>
            @Html.DisplayFor(m => m.COR_VERY_SATISFY_CNT)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">评价满意量:</td>
        <td>
            @Html.DisplayFor(m => m.COR_SATISFY_CNT)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">评价一般量:</td>
        <td>
            @Html.DisplayFor(m => m.COR_COMMON_CNT)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">评价不满意量:</td>
        <td>
            @Html.DisplayFor(m => m.COR_UNSATISFY_CNT)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">未评价量:</td>
        <td>
            @Html.DisplayFor(m => m.COR_NON_EVAL_CNT)
        </td>
    </tr>
</table>