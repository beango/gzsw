@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@using gzsw.model
@using gzsw.model.Enums
@using gzsw.util
@model gzsw.model.CHK_STAFF_APPLYDETAIL

<table class="formTable" cellpadding="0" cellspacing="0">

    <tr>
        <td class="tdLeft">员工名称:</td>
        <td>
            @Html.DisplayFor(m=>m.STAFF_NAM)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">开始时间:</td>
        <td>
            @Model.BEGIN_TIME.ToString("yyyy-MM-dd HH时")
        </td>
    </tr>
    <tr>
        <td class="tdLeft">结束时间:</td>
        <td>
            @Model.END_TIME.ToString("yyyy-MM-dd HH时")
        </td>
    </tr>
    <tr>
        <td class="tdLeft">假期类型:</td>
        <td>
           @(EnumHelper.ConvertToE<HOLLI_TYPE_ENUM>(Model.HOLLI_TYP.ToString()))
        </td>
    </tr>
    <tr>
        <td class="tdLeft">请假原因:</td>
        <td>
            @Html.DisplayFor(m => m.APPLY_REASON)
        </td>
    </tr>
</table>