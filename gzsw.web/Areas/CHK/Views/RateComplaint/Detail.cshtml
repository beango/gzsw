@{
    ViewBag.Title = "查看业务量数据修正";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@using gzsw.controller.CHK.Models
@using gzsw.model
@using gzsw.util
@using gzsw.util.Extensions
@using gzsw.web
@model gzsw.model.CHK_HALL_STAT_M



<table class="formTable" cellpadding="0" cellspacing="0">
    <tr>
        <td style="width: 160px;" class="tdLeft">员工名称:</td>
        <td >
             @ViewBag.HALL_NAM
        </td>
    </tr>
    <tr>
        <td class="tdLeft">月份:</td>
        <td>
             @ViewBag.STAT_MO.ToString().Insert(4, "年")月
        </td>
    </tr>
    <tr>
        <td style="width: 160px;" class="tdLeft">修正后的投诉事件次数:</td>
        <td>
            @Html.DisplayFor(m => m.COR_COMPLAIN_CNT)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">修正后的服务厅全体人数:</td>
        <td>
            @Html.DisplayFor(m => m.COR_HALL_STAFF_CNT)
        </td>
    </tr> 
   
</table>