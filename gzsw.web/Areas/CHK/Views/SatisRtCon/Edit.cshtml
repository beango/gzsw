@{
    ViewBag.Title = "修改满意度";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@using gzsw.web
@model gzsw.controller.CHK.Models.SatisRtConModel

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <table class="formTable" cellpadding="0" cellspacing="0">
        <tr>
            <td class="tdLeft">组织结构：</td>
            <td>
                @Html.DisplayFor(m=>m.ORG_NAM)
                @Html.HiddenFor(m=>m.ORG_ID)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.ORG_ID)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">评价很满意得分：</td>
            <td>
                @Html.TextBoxFor(m => m.VERY_SATISFY_SCORE)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.VERY_SATISFY_SCORE)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">评价满意得分：</td>
            <td>
                @Html.TextBoxFor(m => m.SATISFY_SCORE)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.SATISFY_SCORE)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">评价基本满意得分：</td>
            <td>
                @Html.TextBoxFor(m => m.COMMON_SCORE)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.COMMON_SCORE)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">评价不满意得分：</td>
            <td>
                @Html.TextBoxFor(m => m.UNSATISFY_SCORE)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.UNSATISFY_SCORE)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">未评价得分：</td>
            <td>
                @Html.TextBoxFor(m => m.NON_EVAL_SCORE)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.NON_EVAL_SCORE)
            </td>
        </tr>
        <tr>
            <td colspan="3" style="text-align: center">
                <input id="btnSubmit" type="submit" class="l-button" value="提交" />
            </td>
        </tr>
    </table>
}

@section Head
{
    @Html.JavaScript("lib/jquery.validate/jquery.validate.min.js")
    @Html.JavaScript("lib/jquery.validate/jquery.validate.unobtrusive.min.js")
}
