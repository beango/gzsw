@{
    ViewBag.Title = "查看业务量数据修正";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@using gzsw.controller.CHK.Models
@using gzsw.model
@using gzsw.util
@using gzsw.util.Extensions
@using gzsw.web
@model gzsw.model.CHK_HALL_ITEM_MARK



<table class="formTable" cellpadding="0" cellspacing="0">
    <tr>
        <td style="width: 160px;" class="tdLeft">服务厅名称:</td>
        <td >
             @ViewBag.HALL_NAM
        </td>
    </tr>
    <tr>
        <td class="tdLeft">服务厅编码:</td>
        <td>
             @Model.HALL_CHKITEM_CD
        </td>
    </tr>
    <tr>
        <td style="width: 160px;" class="tdLeft">服务厅考核指标类型:</td>
        <td>
            @Model.CHKITEM_TYP 
        </td>
    </tr>
    <tr>
        <td class="tdLeft">服务厅考核指标:</td>
        <td>
              @ViewBag.HALL_CHKITEM_NAM
        </td>
    </tr> 
    <tr>
        <td style="width: 160px;" class="tdLeft">扣分:</td>
        <td>
            @Model.DEDUCT 
        </td>
    </tr>
    <tr>
        <td class="tdLeft">原因:</td>
        <td>
              @Model.REASON
        </td>
    </tr> 
   
    <tr>
        <td style="width: 160px;" class="tdLeft">打分时间:</td>
        <td>
            @Model.MARK_TIME 
        </td>
    </tr>
    <tr>
        <td class="tdLeft">修改人:</td>
        <td>
              @ViewBag.MODIFY_ID
        </td>
    </tr> 
   
</table>