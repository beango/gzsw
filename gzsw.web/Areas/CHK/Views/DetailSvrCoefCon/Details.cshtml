@{
    ViewBag.Title = "查看明细业务系统";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@using gzsw.web
@model gzsw.controller.CHK.Models.DetailSvrCoefConModel


<table class="formTable" cellpadding="0" cellspacing="0">
    <tr>
        <td class="tdLeft">组织结构：</td>
        <td>
            @Html.DisplayFor(m => m.ORG_NAM)
            @Html.HiddenFor(m => m.ORG_ID)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">业务事项：</td>
        <td>
            @Html.DisplayFor(m => m.SERIALNAME)
            @Html.HiddenFor(m => m.SERIALID)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">业务折合系数：</td>
        <td>
            @Html.DisplayFor(m => m.COEFFICIENT)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">修改人：</td>
        <td>
            @Html.DisplayFor(m => m.USER_NAM)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">修改时间：</td>
        <td>
            @Model.MODIFY_DTIME.ToString("yyyy-MM-dd HH:mm:ss")
        </td>
    </tr>
</table>
