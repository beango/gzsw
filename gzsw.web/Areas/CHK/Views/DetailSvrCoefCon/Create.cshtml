@{
    ViewBag.Title = "添加明细业务系统";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@using gzsw.web
@model gzsw.controller.CHK.Models.DetailSvrCoefConAddModel

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <table class="formTable" cellpadding="0" cellspacing="0">
        <tr>
            <td class="tdLeft">组织结构：</td>
            <td>
                @Html.DropDownListFor(m => m.ORG_ID, ViewBag.Orgs as SelectList, "----请选择----",new { @style="width: 145px;" })
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.ORG_ID)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">业务事项：</td>
            <td>
                @Html.DropDownListFor(m => m.SERIALID, ViewBag.Serials as SelectList, "----请选择----", new { @style = "width: 145px;" })
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.SERIALID)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">业务折合系数：</td>
            <td>
                @Html.TextBoxFor(m => m.COEFFICIENT)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.COEFFICIENT)
            </td>
        </tr>
        <tr>
            <td colspan="3" style="text-align: center">
                <input id="btnSubmit" type="submit" class="l-button" value="提交" />
            </td>
        </tr>
    </table>
}

@section Head
{
    @Html.JavaScript("lib/jquery.validate/jquery.validate.min.js")
    @Html.JavaScript("lib/jquery.validate/jquery.validate.unobtrusive.min.js")
}
