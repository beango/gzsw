@{
    ViewBag.Title = "查看个人星级评定体系";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@using gzsw.controller.CHK.Models
@using gzsw.model.Enums
@using gzsw.util
@using gzsw.util.Extensions
@using gzsw.web
@model gzsw.controller.CHK.Models.StaffStarSystemConModel


<table class="formTable" cellpadding="0" cellspacing="0">
    <tr>
        <td style="width: 220px;" class="tdLeft">组织结构：</td>
        <td>
            @Html.HiddenFor(m => m.ORG_ID)
            @Html.DisplayFor(m => m.ORG_NAM)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">星级评定时间类型：</td>
        <td>
            @(((EvaluationTimeTypeEunm)Model.TIME_DUR_TYP).GetDescription())
        </td>
        <td>
            @Html.ValidationMessageFor(m => m.TIME_DUR_TYP)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">5星级评定分数：</td>
        <td>
            下限 @Html.DisplayFor(m => m.STAR_5_MIN_SCORE)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">4星级评定分数：</td>
        <td>
            上限 @Html.DisplayFor(m => m.STAR_4_MAX_SCORE, new { @style = "width:40px" }) 
            下限 @Html.DisplayFor(m => m.STAR_4_MIN_SCORE, new { @style = "width:40px" }) 
        </td>
    </tr>
    <tr>
        <td class="tdLeft">3星级评定分数：</td>
        <td>
            上限 @Html.DisplayFor(m => m.STAR_3_MAX_SCORE, new { @style = "width:40px" }) 
            下限 @Html.DisplayFor(m => m.STAR_3_MIN_SCORE, new { @style = "width:40px" }) 
        </td>
    </tr>
    <tr>
        <td class="tdLeft">2星级评定分数：</td>
        <td>
            上限 @Html.DisplayFor(m => m.STAR_2_MAX_SCORE, new { @style = "width:40px" }) 
            下限 @Html.DisplayFor(m => m.STAR_2_MIN_SCORE, new { @style = "width:40px" }) 
        </td>
    </tr>
    <tr>
        <td class="tdLeft">1星级评定分数：</td>
        <td >
            上限 @Html.DisplayFor(m => m.STAR_1_MAX_SCORE, new { @style = "width:40px" }) 
            下限 @Html.DisplayFor(m => m.STAR_1_MIN_SCORE, new { @style = "width:40px" }) 
        </td>
    </tr>
    <tr>
        <td class="tdLeft">评价最低分：</td>
        <td>
            @Html.DisplayFor(m => m.EVAL_MIN_SCORE)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">业务表现最低分：</td>
        <td>
            @Html.DisplayFor(m => m.SVR_MIN_SCORE)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">业务质量最低分：</td>
        <td>
            @Html.DisplayFor(m => m.QUALITY_MIN_SCORE)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">业务效率最低分：</td>
        <td>
            @Html.DisplayFor(m => m.EFFIC_MIN_SCORE)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">考勤得分最低分：</td>
        <td>
            @Html.DisplayFor(m => m.ATTEND_MIN_SCORE)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">日常表现最低分：</td>
        <td>
            @Html.DisplayFor(m => m.USU_ACT_MIN_SCORE)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">投诉次数上限：</td>
        <td>
            @Html.DisplayFor(m => m.COMPLAIN_MAX_CNT)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">不满足评定的默认星级：</td>
        <td>
            @(((StarLevelEunm)Model.DEFAULT_STAR).GetDescription())
        </td>
    </tr>
</table>
