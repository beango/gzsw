@{
    ViewBag.Title = "查看业务质量差错修正";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@using gzsw.util.Extensions
@model gzsw.model.Subclasses.STAT_STAFF_QUALITYSTAT_M_SUB



<table class="formTable" cellpadding="0" cellspacing="0">
    <tr>
        <td style="width: 160px;" class="tdLeft">员工名称:</td>
        <td>
            @Html.DisplayFor(m => m.STAFF_NAM)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">月份:</td>
        <td>
            @Model.STAT_MO.ToYearsMonthString()
        </td>
    </tr>
    <tr>
        <td style="width: 160px;" class="tdLeft">质量类型名称:</td>
        <td>
            @Html.DisplayFor(m => m.QUALITY_NAM)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">差错业务笔数:</td>
        <td>
            @Html.DisplayFor(m => m.COR_ERROR_SVR_CNT)
        </td>
    </tr>

</table>