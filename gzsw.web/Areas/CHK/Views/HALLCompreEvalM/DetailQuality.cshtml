@using gzsw.model
@using gzsw.util
@using gzsw.web;
@model PetaPoco.Page<dynamic>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<!-- 查询菜单 --> 

<div class="tablePanel">
    <table class="tablelist">
        <thead>
            <tr>
                 
                <th style="width:38px;">序号 </th>  
                     <th>服务厅编码</th>
                <th>服务厅名称</th>
                <th>员工编码</th>
                <th>员工名称</th>
                <th >事项</th>
                <th >质量类型编码</th>
                <th >数量</th>
                <th >差错发生日期</th> 
            </tr>
        </thead>

        @{
            var i = 1;
        }
        @foreach (var item in Model.Items)
        {
            <tr>
               
                <td>
                    @(i++)
                </td>
                <td>@item.ORG_ID</td>
                <td>@item.HALL_NAM</td>
                <td>@item.STAFF_ID</td>
                <td>@item.STAFF_NAM</td>
                <td>@item.SERIALNAME</td>
                <td>@item.QUALITY_NAM</td>
                <td>@item.AMOUNT</td>
                <td>@item.OCCUR_DT</td> 
            </tr>
        }
    </table>
    @if (Model.Items.Count < 1)
    {
        <div class="l-panel-notDATA">
            暂无数据
        </div>
    }
    @Html.Pager("pageIndex", Model.ItemsPerPage, Model.TotalItems)
</div>

@section Head{
    @Html.JavaScript("lib/datepicker/WdatePicker.js")
     
}
