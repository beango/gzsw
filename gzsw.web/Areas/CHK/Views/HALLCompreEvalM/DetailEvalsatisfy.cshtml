@using gzsw.model
@using gzsw.util
@using gzsw.web;
@model PetaPoco.Page<dynamic>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<!-- 查询菜单 --> 

<div class="tablePanel">
    <table class="tablelist">
        <thead>
            <tr>
                
                <th style="width:38px;">序号 </th>  
                     <th>服务厅编码</th>
                <th>服务厅名称</th>
                <th>员工编码</th>
                <th>员工名称</th>
                <th >呼叫量</th>
                <th >办理量</th>
                <th >弃号量</th>
                <th >同城受理量</th>
                <th >一票多业务量</th>
                <th>评价很满意量</th>
                <th>评价满意量</th> 
                    <th >评价一般量</th>
                <th>评价不满意量</th>
                    <th >未评价量</th> 
            </tr>
        </thead>

        @{
            var i = 1;
        }
        @foreach (var item in Model.Items)
        {
            <tr>
                 
                <td>
                    @(i++)
                </td>
                <td>@item.HALL_NO</td>
                <td>@item.HALL_NAM</td>
                <td>@item.STAFF_ID</td>
                <td>@item.STAFF_NAM</td>
                <td>@item.CALL_CNT</td>
                <td>@item.HANDLE_CNT</td>
                <td>@item.ABANDON_CNT</td>
                <td>@item.LOCAL_CNT</td>
                <td>@item.VOTE_MULTI_CNT</td> 
                <td>@item.VERY_SATISFY_CNT</td> 
                <td>@item.SATISFY_CNT</td> 
                  <td>@item.COMMON_CNT</td> 
                <td>@item.UNSATISFY_CNT</td> 
                <td>@item.NON_EVAL_CNT</td> 
            </tr>
        }
    </table>
    @if (Model.Items.Count < 1)
    {
        <div class="l-panel-notDATA">
            暂无数据
        </div>
    }
    @Html.Pager("pageIndex", Model.ItemsPerPage, Model.TotalItems)
</div>

@section Head{
    @Html.JavaScript("lib/datepicker/WdatePicker.js")
     
}
