@{
    ViewBag.Title = "查看日常行为表现";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@using gzsw.web
@model gzsw.controller.CHK.Models.StaffUsuActConEditModel


<table class="formTable" cellpadding="0" cellspacing="0">
    <tr>
        <td class="tdLeft" style="width: 140px;">组织结构：</td>
        <td>
            @Html.HiddenFor(m => m.ORG_ID)
            @Html.DisplayFor(m => m.ORG_NAM)
        </td>
    </tr>
    <tr>
        <td class="tdLeft">工作纪律项占比：</td>
        <td>
            @Model.WORK_DIS_RT.ToString("P")
        </td>
    </tr>
    <tr>
        <td class="tdLeft">办税服务项占比：</td>
        <td>
            @Model.TAX_SVR_RT.ToString("P")
        </td>
    </tr>
    <tr>
        <td class="tdLeft">税容税貌项占比：</td>
        <td>
            @Model.TAX_LOOK_RT.ToString("P")
        </td>
    </tr>
    <tr>
        <td class="tdLeft">学习培训项占比：</td>
        <td>
            @Model.TRAIN_RT.ToString("P")
        </td>
    </tr>
    <tr>
        <td class="tdLeft">安全卫生项占比：</td>
        <td>
            @Model.SEC_HEAL_RT.ToString("P")
        </td>
    </tr>
    <tr>
        <td class="tdLeft">其他项占比：</td>
        <td>
            @Model.OTHER_RT.ToString("P")
        </td>
    </tr>
</table>



