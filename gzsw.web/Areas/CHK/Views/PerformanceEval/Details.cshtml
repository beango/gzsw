@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@model  gzsw.model.dto.CHK_STAFF_QUALITY_MARKDto
@section Head{
}
<table class="formTable" cellpadding="0" cellspacing="0">
    <tr>
        <td style="width:100px;" class="tdLeft"> 员工编码：</td>
        <td>@Html.DisplayFor(model => model.STAFF_ID)</td>
    </tr>
    <tr>
        <td style="width:100px;" class="tdLeft"> 员工名称：</td>
        <td>@Html.DisplayFor(model => model.PersonName)</td>
    </tr>
    <tr>
        <td class="tdLeft"> 事项编码：</td>
        <td>@Html.DisplayFor(model => model.SERIALID)</td>
    </tr>
    <tr>
        <td class="tdLeft"> 事项名称：</td>
        <td>@Html.DisplayFor(model => model.SERIAL_Name)</td>
    </tr>
    <tr>
        <td class="tdLeft"> 质量类型编码：</td>
        <td>@Html.DisplayFor(model => model.QUALITY_CD)</td>
    </tr>
    <tr>
        <td class="tdLeft"> 质量类型名称：</td>
        <td>@Html.DisplayFor(model => model.QUALITY_Name)</td>
    </tr>
    <tr>
        <td class="tdLeft"> 数量：</td>
        <td>@Html.DisplayFor(model => model.AMOUNT)</td>
    </tr>
    <tr>
        <td class="tdLeft"> 差错发生日期：</td>
        <td>@Html.DisplayFor(model => model.OCCUR_DT)</td>
    </tr> 
    <tr>
        <td class="tdLeft"> 创建人：</td>
        <td>@Html.DisplayFor(model => model.MODIFY_Name)</td>
    </tr> 
    <tr>
        <td class="tdLeft"> 创建时间：</td>
        <td>@Html.DisplayFor(model => model.MODIFY_DTIME)</td>
    </tr> 
</table>

