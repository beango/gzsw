@{
    ViewBag.Title = "修改质量类型";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@using gzsw.web
@model gzsw.model.CHK_QUALITY_CON

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(x=>x.QUALITY_CD)
    <table class="formTable" cellpadding="0" cellspacing="0">
        <tr>
            <td style="width:100px;" class="tdLeft">质量类型编码：</td>
            <td style="width:150px;">@Model.QUALITY_CD</td>
            <td> </td>
        </tr>
        <tr>
            <td class="tdLeft">质量类型名称：</td>
            <td>@Html.EditorFor(model => model.QUALITY_NAM)</td>
            <td>@Html.ValidationMessageFor(model => model.QUALITY_NAM)</td>
        </tr>
        <tr>
            <td class="tdLeft">每笔扣分：</td>
            <td>@Html.EditorFor(model => model.DEDUCT)</td>
            <td>@Html.ValidationMessageFor(model => model.DEDUCT)</td>
        </tr>
        <tr>
            <td class="tdLeft"> &nbsp;  </td>
            <td>
                <input type="submit" class="l-button" value="提交" />
            </td>
            <td> </td>
        </tr>
    </table>
}
@section Head{
    @Html.JavaScript("lib/jquery.validate/jquery.validate.min.js")
    @Html.JavaScript("lib/jquery.validate/jquery.validate.unobtrusive.min.js")
}