@{
    ViewBag.Title = "修改考勤数据修正";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@using gzsw.controller.CHK.Models
@using gzsw.model
@using gzsw.util
@using gzsw.util.Extensions
@using gzsw.web
@model gzsw.controller.CHK.Models.StatStaffChkstatMModel


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <table class="formTable" cellpadding="0" cellspacing="0">
        <tr>
            <td style="width: 160px;" class="tdLeft">员工名称:</td>
            <td>
                @Html.HiddenFor(m => m.STAFF_ID)
                @Html.DisplayFor(m => m.STAFF_NAM)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.STAFF_ID)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">月份:</td>
            <td>
                @Html.HiddenFor(m => m.STAT_MO)
                @Model.STAT_MO.ToYearsMonthString()
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.STAT_MO)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">上班天数:</td>
            <td>
                @Html.TextBoxFor(m => m.COR_WORK_DAY_CNT)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.COR_WORK_DAY_CNT)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">迟到天数:</td>
            <td>
                @Html.TextBoxFor(m => m.COR_LAT_DAY_CNT)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.COR_LAT_DAY_CNT)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">未签退次数:</td>
            <td>
                @Html.TextBoxFor(m => m.COR_NONSIGN_OUT_CNT)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.COR_NONSIGN_OUT_CNT)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">事假天数:</td>
            <td>
                @Html.TextBoxFor(m => m.COR_HOLLI_TYP1_CNT)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.COR_HOLLI_TYP1_CNT)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">病假天数:</td>
            <td>
                @Html.TextBoxFor(m => m.COR_HOLLI_TYP2_CNT)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.COR_EAR_DAY_CNT)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">丧假天数:</td>
            <td>
                @Html.TextBoxFor(m => m.COR_HOLLI_TYP3_CNT)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.COR_HOLLI_TYP3_CNT)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">婚假天数:</td>
            <td>
                @Html.TextBoxFor(m => m.COR_HOLLI_TYP4_CNT)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.COR_HOLLI_TYP4_CNT)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">年休假天数:</td>
            <td>
                @Html.TextBoxFor(m => m.COR_HOLLI_TYP5_CNT)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.COR_HOLLI_TYP5_CNT)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">工伤假天数:</td>
            <td>
                @Html.TextBoxFor(m => m.COR_HOLLI_TYP6_CNT)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.COR_HOLLI_TYP6_CNT)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">加班补休天数:</td>
            <td>
                @Html.TextBoxFor(m => m.COR_HOLLI_TYP7_CNT)
