            </td>
        </tr>
        <tr>
            <td class="tdLeft">产假天数:</td>
            <td>
                @Html.TextBoxFor(m => m.COR_HOLLI_TYP9_CNT)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.COR_HOLLI_TYP9_CNT)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">公差天数:</td>
            <td>
                @Html.TextBoxFor(m => m.COR_HOLLI_TYP10_CNT)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.COR_HOLLI_TYP10_CNT)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">会议天数:</td>
            <td>
                @Html.TextBoxFor(m => m.COR_HOLLI_TYP11_CNT)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.COR_HOLLI_TYP11_CNT)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">学习天数:</td>
