@{
    ViewBag.Title = "修改纳税人评价修正";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@using gzsw.controller.CHK.Models
@using gzsw.model
@using gzsw.util
@using gzsw.util.Extensions
@using gzsw.web
@model CurrqueuehistModel


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <table class="formTable" cellpadding="0" cellspacing="0">
        <tr>
            <td style="width: 160px;" class="tdLeft">服务厅:</td>
            <td>
                @Html.HiddenFor(m => m.CHQUEUE_TRANSCODEID)
                @Html.HiddenFor(m => m.CHQUEUE_SYSNO)
                @Html.DisplayFor(m => m.HALL_NAM)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.CHQUEUE_SYSNO)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">窗口:</td>
            <td>
                @Html.HiddenFor(m => m.CHQUEUE_COUNTER)
                @Html.DisplayFor(m => m.CHQUEUE_COUNTER)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.CHQUEUE_COUNTER)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">业务:</td>
            <td>
                @Html.HiddenFor(m => m.CHQUEUE_QSERIALID)
                @Html.DisplayFor(m => m.Q_SERIALNAME)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.CHQUEUE_QSERIALID)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">操作员工:</td>
            <td>
                @Html.HiddenFor(m => m.CHQUEUE_SNO)
                @Html.DisplayFor(m => m.STAFF_NAM)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.CHQUEUE_SNO)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">纳税人识别码:</td>
            <td>
                @Html.DisplayFor(m => m.CHQUEUE_NSRSBM)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.CHQUEUE_NSRSBM)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">纳税人名称:</td>
            <td>
                @Html.DisplayFor(m => m.CHQUEUE_NSRMC)
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.CHQUEUE_NSRMC)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">评价结果值:</td>
            <td>
                @Html.DropDownListFor(m => m.CHQUEUE_PJRESULT, EnumHelper.GetCategorySelectList(typeof(PJRESULTENUM), false))
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.CHQUEUE_PJRESULT)
            </td>
        </tr>
        <tr>
            <td class="tdLeft">评价时间:</td>
            <td>
                @(Model.CHQUEUE_ETIME != null ? Model.CHQUEUE_ETIME.GetValueOrDefault().ToString("yyyy-MM-dd HH:mm:ss") : "")
            </td>
            <td>
                @Html.ValidationMessageFor(m => m.CHQUEUE_ETIME)
            </td>
        </tr>
        <tr>
            <td colspan="3" style="text-align: center">
                <input id="btnSubmit" type="submit" class="l-button" value="提交" />
            </td>
        </tr>
    </table>
}
@section Head
{
    @Html.JavaScript("lib/jquery.validate/jquery.validate.min.js")
    @Html.JavaScript("lib/jquery.validate/jquery.validate.unobtrusive.min.js")
}