@model gzsw.model.SYS_DLSERIAL

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<table class="formTable" cellpadding="0" cellspacing="0">
        <tr>
            <td style="width:100px;" class="tdLeft">
                大类事项编码：
            </td>
            <td>@Html.DisplayFor(model => model.DLS_SERIALID)</td>
        </tr>

        <tr>
            <td class="tdLeft">
                大类事项名称：
            </td>
            <td>@Html.DisplayFor(model => model.DLS_SERIALNAME)</td>
        </tr>

        <tr>
            <td class="tdLeft">
                备注：
            </td>
            <td>@Html.DisplayFor(model => model.SYS_BZ)</td>
        </tr>
    </table>
